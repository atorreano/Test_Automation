<?xml version="1.0" encoding="utf-8" ?>
<configuration>

  <appSettings>
    <!-- local or remote -->
    <add key="webdriver-type" value="remote"/>

    <!-- browser type -->
    <add key="browser" value="chrome"/>

    <!-- platform, windows or linux -->
    <add key="platform" value="windows"/>

    <!-- location of the selenium grid url -->
    <add key="selenium-grid-url" value="http://localhost:4446/wd/hub"/>

    <!-- The Url of the application under test/check -->
    <add key="site-url" value="http://www.torreano.us"/>



  </appSettings>

</configuration>